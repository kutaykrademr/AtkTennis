<script>

    $(document).ready(function () {

        myfunction();

    });

    function myfunction() {

        $.ajax({
            type: "Get",
            url: '../Public/UserList',
            cache: false,
            dataType: "json",
            async: true,
            success: function (res) {

                debugger;

                var mem = [];

                for (var i = 0; i < res.memberLists.length; i++) {
                    mem.push(res.memberLists[i].memberNumber + ' - ' + res.memberLists[i].fullName + ' - ' + res.memberLists[i].nickName);
                }

                $("#refmem1").autocomplete({
                    source: mem
                });

                $("#refmem2").autocomplete({
                    source: mem
                });
            },

            error: function (textStatus, errorThrown) {
                console.log(JSON.stringify(textStatus));
                console.log(textStatus);
            }
        });
    }

    $(function () {

        $('#datemask').inputmask('dd/mm/yyyy', { 'placeholder': 'dd/mm/yyyy' })
        $('[data-mask]').inputmask()
        $('#reservationdate').datetimepicker({
            format: 'L'
        });
    })

    userObj = {

        name: null,
        username: null,
        startDate: null,
        finishDate: null,
        role: null,
        condition: null,
        identificationNumber: null,
        webReservation: null,
        phone: null,
        phoneExp: null,
        phone2: null,
        phone2Exp: null,
        email: null,
        emailExp: null,
        birthdate: null,
        birthPlace: null,
        motherName: null,
        fatherName: null,
        city: null,
        district: null,
        job: null,
        note: null,
        password: null,
        gender: null,
        nickName: null,
        privRes: null,
        refmem1: null,
        refmem2: null,
        detailAddress: null,
        memberNumber: null,
        memberNumber2: null,
        nickName2: null,
        username2: null,
        webReservation2: null,
        startDate2: null,
        finishDate2: null,
        isPartner: null,
        partnerName: null,
        partnerPhone: null,
        partnerIdNumber: null,
        partnerBirthDate: null,
        password2: null,
        companyId: null,
        memType:null,

    };

    function register() {

        userObj.memType = $("#memType option:selected").val();
        userObj.companyId = $("#compId").text();
        userObj.name = $("#name").val();
        userObj.username = $("#memberNum").val();
        userObj.nickName = $("#nickname").val();
        userObj.startDate = $("#startDate").val();
        userObj.finishDate = $("#finishDate").val();
        userObj.role = "Üye";
        userObj.condition = $("#condition option:selected").val();
        userObj.identificationNumber = $("#identificationNumber").val();
        userObj.webReservation = $("#webReservation option:selected").val();
        userObj.phone = $("#phone").val();
        userObj.phoneExp = $("#phoneExp").val();
        userObj.phone2 = $("#phone2").val();
        userObj.phone2Exp = $("#phone2Exp").val();
        userObj.email = $("#email").val();
        userObj.emailExp = $("#emailExp").val();
        userObj.birthdate = $("#birthdate").val();
        userObj.birthPlace = $("#birthPlace").val();
        userObj.motherName = $("#motherName").val();
        userObj.fatherName = $("#fatherName").val();
        userObj.city = $("#city").val();
        userObj.district = $("#district").val();
        userObj.job = $("#job").val();
        userObj.note = $("#note").val();
        userObj.password = $("#password").val();
        userObj.gender = "Male";
        userObj.memberNumber = $("#memberNum").val();
        userObj.refmem1 = $("#refmem1").val();
        userObj.refmem2 = $("#refmem2").val();
        userObj.detailAddress = $("#detailAddress").val();


   

        if ($('#part').is(":checked")) {

            userObj.nickName2 = $("#nickname2").val();
            userObj.username2 = $("#memberNum2").val();
            userObj.startDate2 = $("#startDate2").val();
            userObj.finishDate2 = $("#finishDate2").val();
            userObj.memberNumber2 = $("#memberNum2").val();
            userObj.password2 = $("#password2").val();
            userObj.isPartner = true;
            userObj.partnerBirthDate = $("#partnerBirthdate").val();
            userObj.partnerIdNumber = $("#partnerIdNumber").val();
            userObj.partnerPhone = $("#partnerPhone").val();
            userObj.partnerName = $("#partnerName").val();
        }


        if ($("#refmem2").val() == "" || $("#refmem1").val() == "" || $("#name").val() == "" || $("#username").val() == "" || $("#nickname").val() == "" || $("#memberNum").val() == "" || $("#memType option:selected").val() == "" || $("#startDate").val() == "" || $("#password").val() == "" || $("#webReservation option:selected").val() == "") {

            alert("Üye Zorunlu Alanları Boş Bırakılmış, Lütfen Eksiksiz Doldurunuz !!!");
        }

        else if ($('#part').is(":checked")) {

            if ($("#partnerBirthdate").val() == "" || $("#partnerPhone").val() == "" || $("#partnerIdNumber").val() == "" || $("#partnerName").val() == "") {

                alert("Eş Bilgisi Seçimi Yapılıp Bilgiler Boş Bırakılmış , Lütfen Bilgileri Doldurunuz !!!");
            }

            else if (userObj.nickName2 == userObj.nickName || userObj.username == userObj.username2 || userObj.memberNumber == userObj.memberNumber2) {

                alert("Eş Bilgisi ve Üye Bilgisi Kullanıcı Adı / Üye Numarası / Rumuz Aynı !! Değişiklik Yapınız !!!");

            }

            else {

                saveUser(userObj);
            }
        }
        else

            saveUser(userObj);

    }

    function saveUser(userObj) {
        try {
            $.ajax({
                type: "Post",
                url: '../AdminHome/Register',
                cache: false,
                data: userObj,
                dataType: "json",
                async: true,
                success: function (res) {

                    if (res.fullName == "Mn") {
                        $.confirm({
                            title: "Uyarı",
                            content: "Üye Numarası önceden Kayıtlı.",
                            buttons: {
                                TAMAM: function () {


                                }
                            }
                        });

                    }

                    else if (res.fullName == "Nn") {

                        $.confirm({
                            title: "Uyarı",
                            content: "Üye Numarası önceden Kayıtlı.",
                            buttons: {
                                TAMAM: function () {


                                }
                            }
                        });

                    }

                    else
                        $.confirm({
                            title: "Uyarı",
                            content: "Kullanıcı Kaydedilirken Hata Oluştu.",
                            buttons: {
                                TAMAM: function () {


                                }
                            }
                        });

                },
                error: function (textStatus, errorThrown) {
                    console.log(JSON.stringify(textStatus));
                    console.log(textStatus);
                }
            });
        } catch (e) {
            console.log(e.message);
        }
    }

    function getNickName() {

        var name = $("#name").val();

        var ad = name.split(" ");

        var ad1 = ad[0][0];
        var soyad1 = ad[1][0];

        var text = "";
        var possible = "0123456789";

        for (var i = 0; i < 3; i++)

            text += possible.charAt(Math.floor(Math.random() * possible.length));

        var nick = (ad1 + soyad1 + text).toLowerCase();


        $.ajax({
            type: "GET",
            url: '../AdminHome/ControlNickName',
            cache: false,
            data: { "nickName": nick },
            dataType: "json",
            async: true,
            success: function (res) {

                if (res == true) {

                    $("#nickname").val(nick);
                }
                else {

                    getNickName()

                }

            },

            error: function (textStatus, errorThrown) {
                console.log(JSON.stringify(textStatus));
                console.log(textStatus);
            }
        });

    }

    function getMemberNumber() {


        var text = "";
