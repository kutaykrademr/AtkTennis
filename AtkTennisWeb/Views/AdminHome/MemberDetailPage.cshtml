@model Helpers.Dto.PartialViewDtos.GetMemberDetailViewDto;
@using Microsoft.AspNetCore.Http;
@inject IHttpContextAccessor HttpContextAccessor;

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    .tdHover:hover {
        background-color: #131da882;
        color: white !important;
    }
</style>


<div class="content-wrapper">
    <section class="content">
        <div class="container-fluid">
            <div class="card" style="border-radius:40px">
                <div class="card-content">
                    <div class="card-body cleartfix">
                        <div class="media align-items-stretch">

                            <div class="align-self-center">
                                <i style="font-size:60px; margin-right:20px ; color:darkmagenta" class="fas fa-id-card"></i>
                            </div>

                            <div class="media-body">
                                <h4>@Model.memberLists.FullName - @Model.memberLists.NickName</h4>
                                <span>@Model.memberLists.UserName</span>
                                <h4 style="display:none" id="userId">@Model.memberLists.UserId</h4>
                            </div>

                            <div class="align-self-center">
                                <h3>Toplam Bakiye: @Model.memberLists.Price ₺<i style="font-size:25px; margin-left:10px ; color:darkmagenta" class="fas fa-tags"></i></h3>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>


    <section class="content">
        <div class="container-fluid">

            <div class="row">
                <div class="col-lg-3 col-6">

                    <div class="small-box bg-info">
                        <div class="inner">
                            <h3 style="font-size:1.2rem">@Model.memberLists.StartDate</h3>

                            <p>Başlangıç Tarihi</p>
                        </div>
                        <div class="icon">
                            <i class="ion ion-bag"></i>
                        </div>
                    </div>
                </div>

                <div class="col-lg-3 col-6">
                    <div class="small-box bg-success">
                        <div class="inner">
                            <h3 style="font-size:1.2rem">@Model.memberLists.FinishDate<sup style="font-size: 20px"></sup></h3>

                            <p>Bitiş Tarihi</p>
                        </div>
                        <div class="icon">
                            <i class="ion ion-stats-bars"></i>
                        </div>
                    </div>
                </div>

                <div class="col-lg-3 col-6">
                    <div class="small-box bg-warning">
                        <div class="inner">
                            <h3 style="font-size:1.2rem">
                                @{
                                    var a = Model.memberLists.Role.ToLower();

                                }
                                @Char.ToUpperInvariant(a[0])@a.Substring(1)
                            </h3>

                            <p>Kullanıcı Yetkisi</p>
                        </div>
                        <div class="icon">
                            <i class="ion ion-person-add"></i>
                        </div>
                    </div>
                </div>

                <div class="col-lg-3 col-6">
                    <div class="small-box bg-danger">
                        <div class="inner">
                            <h3 style="font-size:1.2rem">@Model.memberLists.Condition</h3>

                            <p>Kullanıcı Durumu</p>
                        </div>
                        <div class="icon">
                            <i class="ion ion-pie-graph"></i>
                        </div>
                    </div>
                </div>

            </div>

            <div class="row">
                <div class="col-lg-6">
                    <div class="card card-white register-card-body">
                        <div class="card-header">
                            <h3 style="color:gray" class="card-title">Kişisel Bilgiler</h3>
                        </div>
                        <div class="row">

                            <div class="card-body col-lg-6">
                                <strong><i class="fas fa-book mr-1"></i> Adı - Soyadı / Rolü</strong>

                                <div class="row">
                                    <div class="input-group col-md-7">
                                        <input type="text" class="form-control" placeholder="Adı - Soyadı" id="name" value="@Model.memberLists.FullName" autocomplete="new-password">
                                        <div class="input-group-append">
                                            <div class="input-group-text">
                                                <span class="fas fa-user"></span>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="input-group col-md-5">
                                        <select id="RegistersRole" class="form-control">
                                            <option disabled="disabled" selected>@Model.memberLists.Role</option>
                                            @for (int i = 0; i < Model.AppIdentityRoles.Count(); i++)
                                            {
                                                <option>@Model.AppIdentityRoles[i].Name</option>
                                            }

                                        </select>
                                    </div>
                                </div>



                                <hr>

                                <strong><i class="fas fa-map-marker-alt mr-1"></i> Kullanıcı Adı - Rumuz</strong>

                                <div class="row">

                                    <div class="input-group col-md-6">
                                        <input id="username" placeholder="Kullanıcı Adı" type="text" class="form-control" value="@Model.memberLists.UserName" autocomplete="new-password">
                                        <div class="input-group-append">
                                            <div class="input-group-text">
                                                <span class="fas fa-user"></span>
                                            </div>
                                        </div>
                                    </div>


                                    <div class="input-group col-md-6">
                                        <input id="birthPlace" placeholder="Rumuz" disabled type="text" class="form-control" value="@Model.memberLists.NickName" autocomplete="new-password">
                                        <div class="input-group-append">
                                            <div class="input-group-text">
                                                <span class="fas fa-user"></span>
                                            </div>
                                        </div>
                                    </div>

                                </div>


                                <hr>

                                <strong><i class="fas fa-map-marker-alt mr-1"></i> Başlangıç Tarihi - Bitiş Tarihi</strong>

                                <div class="row">

                                    <div class="input-group col-md-6">
                                        <input placeholder="Başlangıç Tarihi" value="@Model.memberLists.StartDate" type="text" class="form-control" id="startDate" data-inputmask-alias="datetime" data-inputmask-inputformat="dd/mm/yyyy" data-mask="" inputmode="numeric" autocomplete="new-password">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text"><i class="far fa-calendar-alt"></i></span>
                                        </div>
                                    </div>

                                    <div class="input-group col-md-6">

                                        <input placeholder="Bitiş Tarihi" value="@Model.memberLists.FinishDate" type="text" class="form-control" id="finishDate" data-inputmask-alias="datetime" data-inputmask-inputformat="dd/mm/yyyy" data-mask="" inputmode="numeric" autocomplete="new-password">

                                        <div class="input-group-prepend">
                                            <span class="input-group-text"><i class="far fa-calendar-alt"></i></span>
                                        </div>

                                    </div>
                                </div>


                                <hr>

                                <strong><i class="fas fa-pencil-alt mr-1"></i> T.C. Kimlik Numarası</strong>

                                <div class="input-group">
                                    <input type="text" placeholder="TC Kimlik" class="form-control" id="identificationNumber" value="@Model.memberLists.IdentityNumber" autocomplete="new-password">
                                    <div class="input-group-append">
                                        <div class="input-group-text">
                                            <span class="fas fa-user"></span>
                                        </div>
                                    </div>
                                </div>


                                <hr>

                                <strong><i class="fas fa-map-marker-alt mr-1"></i> Doğum Tarihi - Doğum Yeri</strong>

                                <div class="row">

                                    <div class="input-group col-md-6">
                                        <input placeholder="Doğum Tarihi" value="@Model.memberLists.BirthDate" type="text" class="form-control" id="birthdate" data-inputmask-alias="datetime" data-inputmask-inputformat="dd/mm/yyyy" data-mask="" inputmode="numeric" autocomplete="new-password">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text"><i class="far fa-calendar-alt"></i></span>
                                        </div>
                                    </div>

                                    <div class="input-group col-md-6">
                                        <input id="birthPlace" placeholder="Doğum Yeri" type="text" class="form-control" value="@Model.memberLists.BirthPlace" autocomplete="new-password">
                                        <div class="input-group-append">
                                            <div class="input-group-text">
                                                <span class="fas fa-user"></span>
                                            </div>
                                        </div>
                                    </div>

                                </div>


                                <hr>

                                <strong><i class="fas fa-pencil-alt mr-1"></i> Telefon Numarası - Açıklama</strong>

                                <div class="row">
                                    <div class="input-group  col-md-6">
                                        <input placeholder="Telefon No" value="@Model.memberLists.Phone" id="phone" type="text" class="form-control" data-inputmask="&quot;mask&quot;: &quot;(999) 999-9999&quot;" data-mask="" inputmode="text" autocomplete="new-password">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text"><i class="fas fa-phone"></i></span>
                                        </div>
                                    </div>

                                    <div class="input-group  col-md-6">
                                        <input type="text" class="form-control" value="@Model.memberLists.PhoneExp" id="phoneExp" placeholder="Tel Açıklama" autocomplete="new-password">
                                        <div class="input-group-append">
                                            <div class="input-group-text">
                                                <span class="fas fa-user"></span>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div style="margin-top:15px" class="row">
                                    <div class="input-group  col-md-6">
                                        <input placeholder="Telefon No 2" value="@Model.memberLists.Phone2" id="phone2" type="text" class="form-control" data-inputmask="&quot;mask&quot;: &quot;(999) 999-9999&quot;" data-mask="" inputmode="text" autocomplete="new-password">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text"><i class="fas fa-phone"></i></span>
                                        </div>
                                    </div>

                                    <div class="input-group col-md-6">
                                        <input type="text" value="@Model.memberLists.Phone2Exp" class="form-control" id="phone2Exp" placeholder="Tel Açıklama 2" autocomplete="new-password">
                                        <div class="input-group-append">
                                            <div class="input-group-text">
                                                <span class="fas fa-user"></span>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <hr>

                                <strong><i class="fas fa-pencil-alt mr-1"></i> E-Posta Adresi - Açıklama</strong>

                                <div class="row">

                                    <div class="input-group  col-md-8">
                                        <input value="@Model.memberLists.Email" type="email" class="form-control" id="email" placeholder="Email" autocomplete="new-password">
                                        <div class="input-group-append">
                                            <div class="input-group-text">
                                                <span class="fas fa-envelope"></span>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="input-group col-md-4">
                                        <input type="text" value="@Model.memberLists.EmailExp" class="form-control" id="emailExp" placeholder="Email Açıklama" autocomplete="new-password">
                                        <div class="input-group-append">
                                            <div class="input-group-text">
                                                <span class="fas fa-user"></span>
                                            </div>
                                        </div>
                                    </div>

                                </div>

                                <hr>

                                <strong><i class="fas fa-pencil-alt mr-1"></i>Meslek</strong>

                                <div class="input-group col-md-12">
                                    <input type="text" value="@Model.memberLists.Job" class="form-control" id="job" placeholder="Meslek" autocomplete="new-password">
                                    <div class="input-group-append">
                                        <div class="input-group-text">
                                            <span class="fas fa-user"></span>
                                        </div>
                                    </div>
                                </div>

                                <hr>

                            </div>



                            <div class="card-body col-lg-6 ">
                                <strong><i class="fas fa-book mr-1"></i> Anne - Baba Adı</strong>

                                <div class="row">
                                    <div class="input-group col-md-6">
                                        <input type="text" value="@Model.memberLists.MotherName" class="form-control" id="motherName" placeholder="Anne Adı" autocomplete="new-password">
                                        <div class="input-group-append">
                                            <div class="input-group-text">
                                                <span class="fas fa-user"></span>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="input-group col-md-6">
                                        <input type="text" value="@Model.memberLists.FatherName" class="form-control" id="fatherName" placeholder="Baba Adı" autocomplete="new-password">
                                        <div class="input-group-append">
                                            <div class="input-group-text">
                                                <span class="fas fa-user"></span>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <hr>

                                <strong><i class="fas fa-map-marker-alt mr-1"></i> Şehir - İlçe</strong>

                                <div class="row">
                                    <div class="input-group col-md-6">
                                        <input type="text" value="@Model.memberLists.City" class="form-control" id="city" placeholder="Şehir" autocomplete="new-password">
                                        <div class="input-group-append">
                                            <div class="input-group-text">
                                                <span class="fas fa-user"></span>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="input-group col-md-6">
                                        <input type="text" value="@Model.memberLists.District" class="form-control" id="district" placeholder="İlçe" autocomplete="new-password">
                                        <div class="input-group-append">
                                            <div class="input-group-text">
                                                <span class="fas fa-user"></span>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <hr>

                                <strong><i class="far fa-file-alt mr-1"></i> Notlar</strong>

                                <div class="input-group col-md-12">
                                    <input type="text" value="@Model.memberLists.Note" class="form-control" id="note" placeholder="Not" autocomplete="new-password">
                                    <div class="input-group-append">
                                        <div class="input-group-text">
                                            <span class="fas fa-user"></span>
                                        </div>
                                    </div>
                                </div>

                                <hr>

                                <strong><i class="far fa-file-alt mr-1"></i> Detaylı Adres</strong>

                                <div class="input-group col-md-12">
                                    <input type="text" value="@Model.memberLists.DetailAddress" class="form-control" id="detailAddress" placeholder="Adres Detay" autocomplete="new-password">
                                    <div class="input-group-append">
                                        <div class="input-group-text">
                                            <span class="fas fa-user"></span>
                                        </div>
                                    </div>
                                </div>

                                <hr>
                                <strong><i class="far fa-file-alt mr-1"></i>Referans Üyeler</strong>

                                <div class="input-group col-md-12">
                                    <input type="text" value="@Model.memberLists.ReferenceMember1" class="form-control" id="refMem1" placeholder="Referans Üye" autocomplete="new-password">
                                    <div class="input-group-append">
                                        <div class="input-group-text">
                                            <span class="fas fa-user"></span>
                                        </div>
                                    </div>
                                </div>

                                <div style="margin-top:15px" class="input-group col-md-12">
                                    <input type="text" value="@Model.memberLists.ReferenceMember2" class="form-control" id="refMem2" placeholder="Referans Üye 2" autocomplete="new-password">
                                    <div class="input-group-append">
                                        <div class="input-group-text">
                                            <span class="fas fa-user"></span>
                                        </div>
                                    </div>
                                </div>
                                <hr>


                                <strong><i class="far fa-file-alt mr-1"></i>Durumu</strong>


                                <div class="form-group col-md-12">

                                    <select id="condition" class="form-control">
                                        <option disabled="disabled" selected>@Model.memberLists.Condition</option>
                                        <option>Faal</option>
                                        <option>Çıkan</option>
                                        <option>Dondurulmuş</option>
                                        <option>Fahri</option>
                                        <option>İstifa</option>
                                        <option>Onur</option>
                                        <option>Vefat</option>
                                        <option>Belirtilmemiş</option>
                                    </select>

                                    <hr>

                                </div>

                                <strong><i class="far fa-file-alt mr-1"></i>Web Rezervasyon</strong>


                                <div class="form-group col-md-12">

                                    <select id="webReservation" class="form-control">
                                        <option disabled="disabled" selected>@Model.memberLists.WebReservation</option>

                                        <option value="1">Yapabilir</option>
                                        <option value="0">Yapamaz</option>

                                    </select>

                                    <hr>

                                </div>

                                <strong><i class="far fa-file-alt mr-1"></i>Şifre</strong>

                                <div class="input-group col-md-12">
                                    <input type="text" value="@Model.memberLists.Password" class="form-control" id="password" placeholder="Şifre" autocomplete="new-password">
                                    <div class="input-group-append">
                                        <div class="input-group-text">
                                            <span class="fas fa-user"></span>
                                        </div>
                                    </div>
                                </div>

                                <hr>




                            </div>
                            <button class="btn btn-warning" id="@Model.memberLists.UserId" onclick="updateMemberList(this.id)">Kaydet</button>
                        </div>


                    </div>
                </div>

                <div class="col-lg-6">

                    <div class="card card-white">
                        <div class="card-header">

                            <h2 class="card-title">Bakiye Bilgisi - @Model.memberLists.Price ₺ </h2>

                            @*<div class="card-tools">
                                    <button id="@Model.memberLists.UserId" onclick="" style="color:black; border:1px solid " type="button" class="btn btn-tool tdHover">
                                        Bakiye Yükle
                                    </button>
                                </div>*@

                        </div>
                    </div>

                    <div class="card card-white">
                        <div class="card-header">
                            @if (Model.totalPrice == 0)
                            {
                                <h2 class="card-title"><i data-toggle="popover" data-trigger="hover" data-placement="top" data-container="body" data-content="Rezervasyon Detay Gör" style="margin-right:15px; cursor:pointer" class="fas fa-search"></i>Rezervasyon Borcu Yok</h2>
                            }
                            else
                            {
                                <h2 class="card-title"><i data-toggle="popover" data-trigger="hover" data-placement="top" data-container="body" data-content="Rezervasyon Detay Gör" style="margin-right:15px; cursor:pointer" class="fas fa-search"></i>Rezervasyon Borcu - @Model.totalPrice ₺ </h2>
                            }

                            <div class="card-tools">
                                <button type="button" class="btn btn-tool" data-card-widget="collapse" title="Collapse">
                                    <i class="fas fa-minus"></i>
                                </button>
                            </div>

                        </div>
                        <div class="card-body p-0">
                            <table class="table table-bordered bg-green-soft bg-font-green-soft" style="margin:0;">
                                <tbody>
                                    <tr>
                                        <td style="padding:0;text-align:center;">Ödenmeyen</td>
                                        <td style="padding:0;text-align:center;">Ödeme Yapılan</td>
                                        <td style="padding:0;text-align:center;">İptal Edilen</td>
                                        <td style="padding:0;text-align:center;">Genel</td>
                                    </tr>
                                    <tr class="info" style="color:#000;">
                                        <td style="padding:0;text-align:center;">@Convert.ToInt16(Model.reservations.Where(x => x.PriceInf == false).Count()) + @Convert.ToInt16(Model.reservationCancels.Where(x => x.PriceInf == false && x.Procedure == false).Count())</td>
                                        <td style="padding:0;text-align:center;">@Model.reservations.Where(x => x.PriceInf == true).Count() + @Model.reservationCancels.Where(x => x.PriceInf == true && x.Procedure == false).Count()</td>
                                        <td style="padding:0;text-align:center;">@Model.reservationCancels.Count()</td>
                                        <td style="padding:0;text-align:center;">@Model.reservationCancels.Count() + @Model.reservations.Count()</td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>

                    <div class="card card-white">
                        <div class="card-header">
                            @{
                                if (Model.memberDuesInfs.Where(x => x.PriceCondition == false && x.CancelCondition == false).Count() > 0)
                                {
                                    var fPrice = 0;

                                    var priceList = Model.memberDuesInfs.Where(x => x.PriceCondition == false && x.CancelCondition == false).ToList();

                                    foreach (var item in priceList)
                                    {
                                        var lastP = item.DuesPrice;

                                        fPrice = lastP + fPrice;

                                    }

                                    <h3 class="card-title"><i data-toggle="popover" data-trigger="hover" data-placement="top" data-container="body" data-content="Aidat Detay Gör" style="margin-right:15px; cursor:pointer;" class="fas fa-search"></i>Aidat Borcu - @fPrice ₺ </h3>
                                }
                                else
                                {
                                    <h3 class="card-title"><i data-toggle="popover" data-trigger="hover" data-placement="top" data-container="body" data-content="Aidat Detay Gör" style="margin-right:15px; cursor:pointer;" class="fas fa-search"></i>Aidat Borcu Yok</h3>

                                }
                            }

                            <div class="card-tools">
                                <button type="button" class="btn btn-tool" data-card-widget="collapse" title="Collapse">
                                    <i class="fas fa-minus"></i>
                                </button>
                            </div>
                        </div>
                        <div class="card-body p-0">
                            <table class="table table-bordered bg-green-soft bg-font-green-soft" style="margin:0;">
                                <tbody>
                                    <tr>
                                        <td style="padding:0;text-align:center;">Ödenmeyen</td>
                                        <td style="padding:0;text-align:center;">Ödeme Yapılan</td>
                                        <td style="padding:0;text-align:center;">İptal Edilen</td>
                                        <td style="padding:0;text-align:center;">Genel</td>
                                    </tr>
                                    <tr class="info" style="color:#000;">
                                        @{

                                            var xa = Model.memberDuesInfs.Where(x => x.PriceCondition == false && x.CancelCondition == false).Count();
                                            var ya = Model.memberDuesInfs.Where(x => x.PriceCondition == true && x.CancelCondition == false).Count();
                                            var ass = Model.memberDuesInfs.Where(x => x.CancelCondition == true).Count();
                                            var ca = Convert.ToInt32(xa) + Convert.ToInt32(ya) + Convert.ToInt32(ass);
                                        }

                                        <td style="padding:0;text-align:center;">@xa Adet</td>
                                        <td style="padding:0;text-align:center;">@ya Adet</td>
                                        <td style="padding:0;text-align:center;">@ass Adet</td>
                                        <td style="padding:0;text-align:center;">@ca Adet</td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>

                    @{
                        var x = Model.cabinetListUsers.Where(x => x.CabinetUserId == Model.memberLists.UserId).ToList();
                    }

                    @if (x.Count == 0)
                    {
                        <div class="card card-white">
                            <div class="card-header">
                                <h3 class="card-title">Dolap Bilgileri</h3>

                                <div class="card-tools">
                                    <button id="@Model.memberLists.UserId" onclick="addCabinetforMember(this.id)" style="color:black; border:1px solid" type="button" class="btn btn-tool tdHover">
                                        Dolap Tahsis Et
                                    </button>
                                </div>



                            </div>
                            <div class="card-body p-0">
                                <h5 style="text-align:center">Dolap Bilgisi Bulunmamaktadır.</h5>
                            </div>
                        </div>
                    }

                    else
                    {
                        <div class="card card-white">
                            <div class="card-header">
                                @{
                                    var firstPrice = 0;

                                    foreach (var item in x)
                                    {
                                        if (item.PriceCondition == false)
                                        {
                                            var priceModel = item.CabinetOpPrice;

                                            firstPrice = firstPrice + priceModel;
                                        }

                                    }
                                }

                                <h3 class="card-title"><i data-toggle="popover" data-trigger="hover" data-placement="top" data-container="body" data-content="Dolap Detay Gör" style="margin-right:15px; cursor:pointer" class="fas fa-search"></i>Dolap Bilgileri - @firstPrice ₺ </h3>


                                <div class="card-tools">
                                    <button id="@Model.memberLists.UserId" onclick="addCabinetforMember(this.id)" style="color:black; border:1px solid " type="button" class="btn btn-tool tdHover">
                                        Dolap Tahsis Et
                                    </button>
                                </div>

                            </div>

