<script>

    $(document).ready(function () {

        $('#example2').DataTable({
            "paging": true,
            "lengthChange": false,
            "searching": true,
            "ordering": true,
            "info": true,
            "autoWidth": false,
            "responsive": true,
            "pageLength": 5,
            "buttons": ["print"]
        })
            .buttons().container().appendTo('#example2_wrapper .col-md-6:eq(0)');



    });
    function getNickName2() {

        var name = $("#partnerName").val();

        var ad = name.split(" ");

        var ad1 = ad[0][0];
        var soyad1 = ad[1][0];

        var text = "";
        var possible = "0123456789";

        for (var i = 0; i < 3; i++)

            text += possible.charAt(Math.floor(Math.random() * possible.length));

        var nick = (ad1 + soyad1 + text).toLowerCase();


        $.ajax({
            type: "GET",
            url: '../AdminHome/ControlNickName',
            cache: false,
            data: { "nickName": nick },
            dataType: "json",
            async: true,
            success: function (res) {

                if (res == true) {

                    $("#nickname2").val(nick);
                }
                else {

                    getNickName()

                }

            },

            error: function (textStatus, errorThrown) {
                console.log(JSON.stringify(textStatus));
                console.log(textStatus);
            }
        });

    }

    function getMemberNumber2() {


        var text = "";
        var possible = "0123456789";

        for (var i = 0; i < 5; i++)

            text += possible.charAt(Math.floor(Math.random() * possible.length));

        var memberNumber = text;

        $.ajax({
            type: "GET",
            url: '../AdminHome/ControlMemberNumber',
            cache: false,
            data: { "memberNumber": memberNumber },
            dataType: "json",
            async: true,
            success: function (res) {

                if (res == true) {

                    $("#memberNum2").val(memberNumber);
                }
                else {

                    getMemberNumber()

                }

            },

            error: function (textStatus, errorThrown) {
                console.log(JSON.stringify(textStatus));
                console.log(textStatus);
            }
        });

    }

    function getPassword2() {

        var name = $("#partnerName").val();

        var ad = name.split(" ");

        var ad1 = (ad[0][0]).toUpperCase();
        var soyad1 = (ad[1][0]).toLowerCase();

        var text = "123";
        var char = ".";

        var password = (ad1 + soyad1 + text + char);

        $("#password2").val(password);

    }

    function getUserName2() {


        var text = "";
        var possible = "0123456789";

        for (var i = 0; i < 5; i++)

            text += possible.charAt(Math.floor(Math.random() * possible.length));

        var userName = text;

        $.ajax({
            type: "GET",
            url: '../AdminHome/ControlUserName',
            cache: false,
            data: { "userName": userName },
            dataType: "json",
            async: true,
            success: function (res) {

                if (res == true) {

                    $("#username2").val(userName);
                }
                else {

                    getUserName()

                }

            },

            error: function (textStatus, errorThrown) {
                console.log(JSON.stringify(textStatus));
                console.log(textStatus);
            }
        });

    }



    userObj = {

    

        memberNumber2: null,
        nickName2: null,
        username2: null,     
        startDate2: null,
        finishDate2: null,
        partnerName: null,
        partnerPhone: null,
        partnerIdNumber: null,
        partnerBirthDate: null,
        password2: null,
        id:null,

    };

   
    function addPartner(id) {

        $('#addPartnerModal').modal('toggle');

        $("#addPartOp button:nth-of-type(2)").attr("id", id);
 
    }

    function addOpPartner(id) {
        userObj.id = id;
        userObj.nickName2 = $("#nickname2").val();
        userObj.username2 = $("#username2").val();
        userObj.startDate2 = $("#startDate2").val();
        userObj.finishDate2 = $("#finishDate2").val();
        userObj.memberNumber2 = $("#memberNum2").val();
        userObj.password2 = $("#password2").val();
        userObj.partnerBirthDate = $("#partnerBirthdate").val();
        userObj.partnerIdNumber = $("#partnerIdNumber").val();
        userObj.partnerPhone = $("#partnerPhone").val();
        userObj.partnerName = $("#partnerName").val();

        $.ajax({
            type: "Get",
            url: '../AdminHome/AddPartner',
            cache: false,
            data: userObj,
            dataType: "json",
            async: true,
            success: function (res) {

                if (res != null) {
                    alert("başarılı");
                }
            },

            error: function (textStatus, errorThrown) {
                console.log(JSON.stringify(textStatus));
                console.log(textStatus);
            }
        });
    }

    $(function () {

        $('#datemask').inputmask('dd/mm/yyyy', { 'placeholder': 'dd/mm/yyyy' })
        $('[data-mask]').inputmask()
        $('#reservationdate').datetimepicker({
            format: 'L'
        });
    })

    $(function () {
        $('[data-toggle="popover"]').popover()
    })

    function addCabinetforMember() {


        $('#myModal').modal('toggle');

        $('#cabinetCode').on('change', function () {

            debugger;

            var code = $("#cabinetCode option:selected").val();

            $.ajax({
                type: "Get",
                url: '../AdminHome/GetCabinet',
                cache: false,
                data: { "code": code },
                dataType: "json",
                async: true,
                success: function (res) {


                    $("#cabinetPrice").val(res.cabinetTypes[0].cabinetTypesPrice);
                    $("#cabinetType").val(res.cabinetTypes[0].cabinetTypes);

                },

                error: function (textStatus, errorThrown) {
                    console.log(JSON.stringify(textStatus));
                    console.log(textStatus);
                }
            });

        });

    }

    function addCabinetMem() {

        var userId = $("#userId").text();
        var price = $("#cabinetPrice").val();
        var type = $("#cabinetType").val();
        var code = $("#cabinetCode option:selected").val();
        var who = $("#whoCabinet option:selected").val();

        debugger;

        if (userId == "" && price == "" && type == "" && type == "" && code == "" && who == "") {
            swal("Başarısız!", "Lütfen Bilgileri Doldurunuz", "warning");
        }

        else {
            $.ajax({
                type: "Get",
                url: '../AdminHome/AddCabinet',
                cache: false,
                data: { "code": code, "type": type, "price": price, "who": who, "userId": userId },
                dataType: "json",
                async: true,
                success: function (res) {

                    if (res == false) {
                        swal("Başarısız!", "Bir Hata Oluştu!", "warning");
                    }
                    else {
                        swal({
                            title: "Başarılı",
                            text: "Dolap Tahsis Edildi !",
                            icon: "success",
                            button: "Tamam",
                        }).then((value) => {



                            location.reload();

                        });
                    }

                },

                error: function (textStatus, errorThrown) {
                    console.log(JSON.stringify(textStatus));
                    console.log(textStatus);
                }
            });
        }

    }

    function deleteCabinet(id) {


        if (id == "") {
            swal("Başarısız!", "Bir Hata Oluştu", "warning");
        }
        else {
            swal({
                title: "Emin Misiniz?",
                text: "Dolap  İptal Edilecek !",
                icon: "warning",
                buttons: true,
                dangerMode: true,
            })
                .then((willDelete) => {
                    if (willDelete) {

                        $.ajax({
                            type: "Get",
                            url: '../AdminHome/DeleteCabinets',
                            cache: false,
                            data: { "id": id },
                            dataType: "json",
                            async: true,
                            success: function (res) {


                                if (res == false) {
                                    swal("Başarısız!", "Bir Hata Oluştu!", "warning");
                                }
                                else {
                                    swal({
                                        title: "Başarılı",
                                        text: "Dolap  Başarılı Bir Şekilde Üyeden Alındı !",
                                        icon: "success",
                                        button: "Tamam",
                                    }).then((value) => {

                                        location.reload();

                                    });

                                }

                            },

                            error: function (textStatus, errorThrown) {
                                console.log(JSON.stringify(textStatus));
                                console.log(textStatus);
                            }
                        });

                    }
                    else
                        swal({
                            title: "Dolap Korundu",
                            text: "Dolap Silmekten Vazgeçtiniz!",
                            icon: "success",
                            button: "Tamam",
                        });
                });
        }
    }

    function updateMemberList(id) {

        userObj = {

            id: null,
            name: null,
            username: null,
            startDate: null,
            finishDate: null,
            role: null,
            condition: null,
            identificationNumber: null,
            webReservation: null,
            phone: null,
            phoneExp: null,
            phone2: null,
            phone2Exp: null,
            email: null,
            emailExp: null,
            birthdate: null,
            birthPlace: null,
            motherName: null,
            fatherName: null,
            city: null,
            district: null,
            job: null,
            note: null,
            password: null,
            gender: null,
            checkpass: null,

        };

        debugger;

        userObj.checkpass = $("#password").val();
        userObj.id = id;
        userObj.name = $("#name").val();
        userObj.username = $("#username").val();
        userObj.startDate = $("#startDate").val();
        userObj.finishDate = $("#finishDate").val();
        userObj.role = $("#RegistersRole option:selected").text();
        userObj.condition = $("#condition option:selected").text();
        userObj.identificationNumber = $("#identificationNumber").val();
        userObj.webReservation = $("#webReservation option:selected").val();
        userObj.phone = $("#phone").val();
        userObj.phoneExp = $("#phoneExp").val();
        userObj.phone2 = $("#phone2").val();
        userObj.phone2Exp = $("#phone2Exp").val();
        userObj.email = $("#email").val();
        userObj.emailExp = $("#emailExp").val();
        userObj.birthdate = $("#birthdate").val();
        userObj.birthPlace = $("#birthPlace").val();
        userObj.motherName = $("#motherName").val();
        userObj.fatherName = $("#fatherName").val();
        userObj.city = $("#city").val();
        userObj.district = $("#district").val();
        userObj.job = $("#job").val();
        userObj.note = $("#note").val();
        userObj.password = $("#password").val();
        userObj.gender = "Male";



        if (userObj.password != "" || userObj.username != "") {

            try {
                $.ajax({
                    type: "Post",
                    url: '../AdminHome/UpdateMemberList',
                    cache: false,
                    data: userObj,
                    dataType: "json",
                    async: true,
                    success: function (res) {



                        if (res == true) {


                            $.confirm({
                                title: "Bilgilendirme",
                                content: "Kullanıcı Başarıyla Güncellendi.",
                                buttons: {
                                    TAMAM: function () {

                                        location.reload();

                                    }
                                }
                            });
                        }

                        else
                            $.confirm({
                                title: "Uyarı",
                                content: "Kullanıcı Güncellenirken Hata Oluştu. Şifre : En az bir büyük harf , Küçük Harf , Sayı ve Sembol(*/.) içermelidir.",
                                buttons: {
                                    TAMAM: function () {


                                    }
                                }
                            });

                    },
                    error: function (textStatus, errorThrown) {
                        console.log(JSON.stringify(textStatus));
                        console.log(textStatus);
                    }
                });
            } catch (e) {
                console.log(e.message);
            }
        }
        else {
            $.confirm({
                title: "Uyarı",
                content: "Kullanıcı Adı / Şifre Boş Bırakılamaz",
                buttons: {
                    TAMAM: function () {


                    }
                }
            });

        }

    }


</script>

